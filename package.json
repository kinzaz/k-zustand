{
  "name": "k-zustand",
  "version": "1.1.0",
  "main": "./index.js",
  "type": "commonjs",
  "types": "./index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./esm/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./esm/*.d.mts",
        "default": "./esm/*.mjs"
      },
      "default": {
        "types": "./*.d.ts",
        "default": "./*.js"
      }
    }
  },
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "rollup -c"
  },
  "author": "kinzaz",
  "license": "MIT",
  "description": "Mini version of zustand state manager",
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.5",
    "jsdom": "^25.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.21.2",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
